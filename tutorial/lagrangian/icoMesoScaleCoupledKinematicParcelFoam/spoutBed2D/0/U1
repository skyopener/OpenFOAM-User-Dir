FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 39);
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value (0 0 0);
    }

    fixedWalls
    {
        type            slip;
    }
}

